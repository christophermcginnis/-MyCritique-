<% if (success && success.length) {%>
<div class="validation-feedback" role="alert">
    <p style="color: rgb(0, 255, 30)" class="validation-feedback-error"> <%= success %></p>
</div>
<% } %>

<% if (error && error.length) {%>
    <div class="validation-feedback" role="alert">
        <p style="color: rgb(255, 0, 0)" class="validation-feedback-error"> <%= error %></p>
    </div>
    <% } %>